// Funções da tabela PAI X FILHO
function addItem(nomeTabela) {
    FLUIGIP.TABLE.addTableChild(nomeTabela);
}

function clearItens(nomeTabela) {
    FLUIGIP.TABLE.clearTable(nomeTabela, "Todos os itens foram excluídos!");
}

